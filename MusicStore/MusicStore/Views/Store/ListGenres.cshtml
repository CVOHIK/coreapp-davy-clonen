@model IEnumerable<MusicStore.Models.Genre>
@{
    ViewData["Title"] = "Genres";
}

<h2>Genres</h2>
<h3>Select</h3>
<p>Name</p>
<hr />
<table style="list-style: none;">
    @foreach(var item in Model)
    {
    <tr>
        <td>
            <a asp-controller="Store" asp-action="ListAlbums" asp-route-id="@item.GenreID">
                @item.Name
            </a>
        </td>
        <td><br /></td>
    </tr> 
    }
</table>

